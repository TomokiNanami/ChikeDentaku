<?xml version="1.0" encoding="utf-8"?><resources >    <declare-styleable name="NumberButton">        <attr name="number" format="integer" />        <attr name="numberButtonStyle" format="reference" />    </declare-styleable>    <declare-styleable name="FunctionButton">        <attr name="type" format="reference">            <enum name="tax" value="0" />            <enum name="memory_plus" value="1" />            <enum name="memory_minus" value="2" />            <enum name="memory_clear" value="3" />            <enum name="num_dot" value="9" />            <enum name="num_00" value="10" />            <enum name="num_delete" value="11" />            <enum name="op_add" value="20" />            <enum name="op_sub" value="21" />            <enum name="op_multiple" value="22" />            <enum name="op_division" value="23" />            <enum name="op_equal" value="24" />            <enum name="cancel" value="30" />            <enum name="cancel_all" value="31" />        </attr>        <attr name="functionButtonStyle" format="reference" />    </declare-styleable></resources>